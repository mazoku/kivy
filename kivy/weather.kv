AddLocationForm:

<AddLocationForm>:
    orientation: "vertical"
    BoxLayout:
        height: "40dp"
        size_hint_y: None
        TextInput:
            size_hint_x: 50
        Button:
            text: "Test"
            size_hint_x: 25
            on_press: print(self.size)
        Button:
            text: "Search"
            size_hint_x: 25
            on_press: root.search_location()
        Button:
            text: "Current Location"
            size_hint_x: 25
            on_press: app.current_location()
    ListView:
        item_strings: ["Palo Alto, MX", "Palo Alto, US"]