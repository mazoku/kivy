AddLocationForm:

<AddLocationForm>:
    orientation: "vertical"
    search_input: search_box  # Kivy property: id
    search_results: search_results_list

    BoxLayout:
        height: "40dp"
        size_hint_y: None

        TextInput:
            id: search_box
            size_hint_x: 50

        #Button:
        #    text: "Test"
        #    size_hint_x: 25
        #    on_press: print(self.size)  # self = rightmost indented object

        Button:
            text: "Search"
            size_hint_x: 25
            on_press: root.search_location()  # root = leftmost indented object

        Button:
            text: "Current Location"
            size_hint_x: 25
            on_press: app.current_location()  # app = subclass of App on which the run method was called

    ListView:
        id: search_results_list
        item_strings: []